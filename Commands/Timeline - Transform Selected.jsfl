var doc = fl.getDocumentDOM();var tl = doc.getTimeline();var trace = fl.trace;var dialogBox = "Commands/Timeline - Transform Selected.xml";var selectedFrames = tl.getSelectedFrames();var layer = selectedFrames[0];var startFrame = selectedFrames[1];var endFrame = selectedFrames[2];if (selectedFrames.length <= 0) {	alert ('Please select an object.' );} else {	settings = doc.xmlPanel(fl.configURI + dialogBox);	if (settings.dismiss == 'accept'){		var layer = 0;		var startFrame = 1;		var endFrame = 2;		for (var i=0; i<(selectedFrames.length/3); i++){			for (var n=selectedFrames[startFrame]; n<selectedFrames[endFrame]; n++){				var frame = tl.layers[selectedFrames[layer]].frames[n];				if (frame.elements.length > 0){					if (settings._scale == "true") {						frame.elements[0].scaleX = Number (settings._wid) / 100;						frame.elements[0].scaleY = Number (settings._hei) / 100;					} else {						frame.elements[0].width = Number (settings._wid);						frame.elements[0].height = Number (settings._hei);					}				}			}			layer += 3;			startFrame += 3;			endFrame += 3;		}	}}