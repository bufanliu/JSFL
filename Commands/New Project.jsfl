var doc = fl.getDocumentDOM();var settings = doc.xmlPanel(fl.configURI + "Commands/New Project.xml");	if (settings.dismiss == "accept") {	newProject( settings._str );}function newProject( projectName ){	var folderURI = fl.browseForFolderURL ("Select a save location for your project.");	if( folderURI != null && folderURI != "null" ) {				createFolder( folderURI, projectName );				folderURI += "/" + projectName;		createFolder( folderURI, "Flash" );				folderURI += "/Flash";		createFolder( folderURI, "bin" );		createFolder( folderURI, "fla" );		createFolder( folderURI, "lib" );		createFolder( folderURI, "src" );	}}function createFolder( begin, folderName ){	FLfile.createFolder( begin + "/" + folderName );}